{
  "WordPress": {
    "cats": [1, 6],
    "headers": {
      "X-Powered-By": "WordPress"
    },
    "html": [
      "<link[^>]+href=\"[^\"]*wp-content/",
      "<script[^>]+src=\"[^\"]*wp-includes/"
    ],
    "scripts": [
      "/wp-includes/js/",
      "/wp-content/plugins/"
    ],
    "implies": "PHP",
    "icon": "WordPress.svg",
    "website": "https://wordpress.org"
  },
  "Joomla": {
    "cats": [1, 6],
    "headers": {
      "X-Powered-By": "Joomla"
    },
    "html": [
      "Joomla!",
      "com_"
    ],
    "meta": {
      "generator": "Joomla"
    },
    "implies": "PHP",
    "icon": "Joomla.svg",
    "website": "https://www.joomla.org"
  },
  "Drupal": {
    "cats": [1, 6],
    "headers": {
      "X-Drupal-Cache": ""
    },
    "html": [
      "Drupal",
      "drupal"
    ],
    "scripts": [
      "/sites/all/",
      "/modules/"
    ],
    "implies": "PHP",
    "icon": "Drupal.svg",
    "website": "https://www.drupal.org"
  }
}
